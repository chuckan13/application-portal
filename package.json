{
  "name": "app-portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/dist/app.js",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "prod": "yarn run heroku-postbuild && yarn start",
    "server": "nodemon --watch ./server --exec babel-node ./server/bin/www",
    "client": "cd client && yarn start",
    "heroku-postbuild": "yarn build-server && cd client && yarn install && yarn run build",
    "build-server": "babel server --out-dir ./server/dist"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "cas-authentication": "^0.0.8",
    "clean-css": "^4.1.11",
    "constantinople": "^4.0.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "jade": "^1.11.0",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.12",
    "minimist": "^1.2.3",
    "mixin-deep": "^1.3.2",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "sequelize": "^5.21.12",
    "sequelize-cli": "^5.5.1",
    "serve-favicon": "~2.4.5",
    "set-value": "^2.0.1",
    "transformers": "^3.1.0",
    "uglify-js": "^3.9.4",
    "websocket-extensions": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "concurrently": "^3.5.1",
    "nodemon": "^1.18.9"
  }
}
